<T3DataStructure>
<meta>
   <langDisable>1</langDisable>
</meta>
<sheets>
  <sDEF>
    <ROOT>
      <TCEforms>
         <sheetTitle>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar_conf</sheetTitle>
      </TCEforms>
      <type>array</type>
      <el>
   		<cal_type>
   			<TCEforms>
   				<label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.cal_type</label>
   				<config>
      				<type>radio</type>
                  <items type="array">
                  	<numIndex index="0" type="array">
              	         <numIndex index="0">LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar_entries.type.I.1</numIndex>
                 	      <numIndex index="1">1</numIndex>
                  	</numIndex>
                  	<numIndex index="1" type="array">
               	      <numIndex index="0">LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar_entries.type.I.2</numIndex>
                 	      <numIndex index="1">2</numIndex>
                  	</numIndex>
                  </items>
   				</config>
   			</TCEforms>
   		</cal_type>

         <showCats>
   			<TCEforms>
   				<label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.showCats</label>
   				<config>
   					<type>radio</type>
                  <items type="array">
                  	<numIndex index="0" type="array">
                  	   <numIndex index="0">LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.clicksno</numIndex>
                  	   <numIndex index="1">0</numIndex>
                  	</numIndex>
                  	<numIndex index="1" type="array">
                  	   <numIndex index="0">LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.clicksyes</numIndex>
                  	   <numIndex index="1">1</numIndex>
                  	</numIndex>
                  </items>
   					<maxitems>1</maxitems>
   					<size>1</size>
   				</config>
   			</TCEforms>
   		</showCats>

			<categorySelection>
				<TCEforms>
					<label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.category_selection</label>
					<config>
						<type>select</type>
						<items type="array">
							<numIndex index="100">
								<numIndex index="0">LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.category_selection_notCategorized</numIndex>
								<numIndex index="1">0</numIndex>
							</numIndex>
						</items>
						<foreign_table>tx_kkcalendar_cat</foreign_table>
						<foreign_table_where>ORDER BY tx_kkcalendar_cat.uid</foreign_table_where>
						<foreign_table_where>AND tx_kkcalendar_cat.pid=###STORAGE_PID### ORDER BY tx_kkcalendar_cat.title</foreign_table_where>
						<minitems>0</minitems>
						<maxitems>20</maxitems>
						<size>5</size>
						<selectedListStyle>width:150px</selectedListStyle>
						<itemListStyle>width:150px</itemListStyle>
						<allowNonIdValues>1</allowNonIdValues>
					</config>
				</TCEforms>
			</categorySelection>

   		<maxAge>
   			<TCEforms>
               <label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.maxAge</label>
   				<config>
   					<type>input</type>
   					<size>3</size>
   				</config>
   			</TCEforms>
   		</maxAge>

   		<maxDatesInView>
   			<TCEforms>
               <label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.maxDatesInView</label>
   				<config>
   					<type>input</type>
   					<size>3</size>
   				</config>
   			</TCEforms>
   		</maxDatesInView>

   		<noEntryMessage>
   			<TCEforms>
               <label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.noEntryMessage</label>
   				<config>
   					<type>input</type>
   					<size>255</size>
   				</config>
   			</TCEforms>
   		</noEntryMessage>

   		<template_file>
   			<TCEforms>
   				<label>LLL:EXT:kk_calendar/locallang_db.xml:tx_kkcalendar.template_file</label>
   				<config>
   					<type>group</type>
   					<internal_type>file</internal_type>
   					<allowed>tmpl,html,htm</allowed>
   					<max_size>100</max_size>
   					<uploadfolder>uploads/tx_kkcalendar</uploadfolder>
   					<maxitems>1</maxitems>
   					<size>1</size>
   					<selectedListStyle>Width:180px</selectedListStyle>
   				</config>
   			</TCEforms>
   		</template_file>
      </el>
    </ROOT>
  </sDEF>
</sheets>

</T3DataStructure>
