<T3DataStructure>
    <sheets>
	<sDEF>
	    <ROOT>
		<TCEforms>
		    <sheetTitle>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.sheet_general</sheetTitle>
		</TCEforms>
	    <type>array</type>
	    <el>
		<viewmode>
		    <TCEforms>
			<label>View mode</label>
			<config>
			    <type>select</type>
			    <items type="array">
				<numIndex index="0" type="array">
			    	    <numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.defaultview_month</numIndex>
			    	    <numIndex index="1">0</numIndex>
			    	</numIndex>
			    	<numIndex index="1" type="array">
			    	    <numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.defaultview_week</numIndex>
			    	    <numIndex index="1">1</numIndex>
			    	</numIndex>
			    	<numIndex index="2" type="array">
			    	    <numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.defaultview_day</numIndex>
			    	    <numIndex index="1">2</numIndex>
			    	</numIndex>
			    	<numIndex index="3" type="array">
			    	    <numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.defaultview_upcoming</numIndex>
			    	    <numIndex index="1">3</numIndex>
			    	</numIndex>
			    	<numIndex index="4" type="array">
			    	    <numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.defaultview_event</numIndex>
			    	    <numIndex index="1">4</numIndex>
			    	</numIndex>
			    </items>
			    <minitems>1</minitems>
			    <maxitems>1</maxitems>
			    <size>1</size>
			</config>
		    </TCEforms>
		</viewmode>		 
		<ignore_pivars_date>
		    <TCEforms>
			<label>Ignore GET-variables for to switch date?</label>
			<config>
			    <type>check</type>
			</config>
		    </TCEforms>
		</ignore_pivars_date>		 
		<typoscript_prefix>
		    <TCEforms>
			<label>Typoscript Prefix (leave empty for default)</label>
			<config>
			    <type>input</type>
			    <eval>string</eval>
			    <size>30</size>
			</config>
		    </TCEforms>
		</typoscript_prefix>		 
		<date_offset>
		    <TCEforms>
			<label>Date offset (PHP strtotime() format)</label>
			<config>
			    <type>input</type>
			    <eval>string</eval>
			    <size>30</size>
			</config>
		    </TCEforms>
		</date_offset>		 
