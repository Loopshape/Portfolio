		<month_page>
		    <TCEforms>
		        <exclude>1</exclude>
		        <label>Month page</label>
		        <config>
			   <type>group</type>
			   <internal_type>db</internal_type>
			   <allowed>pages</allowed>
			   <size>1</size>
			   <maxitems>1</maxitems>
			   <minitems>0</minitems>
			   <show_thumbs>0</show_thumbs>
		        </config>
		    </TCEforms>
		</month_page>
		<week_page>
		    <TCEforms>
		        <exclude>1</exclude>
		        <label>Week page</label>
		        <config>
			   <type>group</type>
			   <internal_type>db</internal_type>
			   <allowed>pages</allowed>
			   <size>1</size>
			   <maxitems>1</maxitems>
			   <minitems>0</minitems>
			   <show_thumbs>0</show_thumbs>
		        </config>
		    </TCEforms>
		</week_page>
		<day_page>
		    <TCEforms>
		        <exclude>1</exclude>
		        <label>Day page</label>
		        <config>
			   <type>group</type>
		    	   <internal_type>db</internal_type>
		    	   <allowed>pages</allowed>
		    	   <size>1</size>
		    	   <maxitems>1</maxitems>
		    	   <minitems>0</minitems>
		    	   <show_thumbs>0</show_thumbs>
		        </config>
		    </TCEforms>
		</day_page>
		<event_page>
		    <TCEforms>
		        <exclude>1</exclude>
		        <label>Event page</label>
		        <config>
		    	<type>group</type>
		    	<internal_type>db</internal_type>
		    	<allowed>pages</allowed>
		    	<size>1</size>
		    	<maxitems>1</maxitems>
		    	<minitems>0</minitems>
		    	<show_thumbs>0</show_thumbs>
		        </config>
		    </TCEforms>
		</event_page>
 		<pages>
		    <TCEforms>
			<exclude>1</exclude>
			<label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
			<config>
			    <type>group</type>
			    <internal_type>db</internal_type>
			    <allowed>pages</allowed>
			    <size>3</size>
			    <maxitems>22</maxitems>
			    <minitems>0</minitems>
			    <show_thumbs>1</show_thumbs>
			</config>
		    </TCEforms>
		</pages>
		<recursive>
		    <TCEforms>
			<label>LLL:EXT:lang/locallang_general.php:LGL.recursive</label>
			<config>
			    <type>select</type>
			    <items type="array">
				<numIndex index="0" type="array">
				    <numIndex index="0"></numIndex>
				    <numIndex index="1"></numIndex>
				</numIndex>					
				<numIndex index="1" type="array">
				    <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.1</numIndex>
				    <numIndex index="1">1</numIndex>
				</numIndex>
				<numIndex index="2" type="array">
				    <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.2</numIndex>
				    <numIndex index="1">2</numIndex>
				</numIndex>
				<numIndex index="3" type="array">
				    <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.3</numIndex>
				    <numIndex index="1">3</numIndex>
				</numIndex>
				<numIndex index="4" type="array">
				    <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.4</numIndex>
				    <numIndex index="1">4</numIndex>
				</numIndex>
				<numIndex index="5" type="array">
				    <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.5</numIndex>
				    <numIndex index="1">250</numIndex>
				</numIndex>
			    </items>
			    <minitems>0</minitems>
			    <maxitems>1</maxitems>
			    <size>1</size>
			</config>
		    </TCEforms>
		</recursive>  
	    </el>
	</ROOT>
	    <!-- xXX offset -->
	    <!-- unique CSS marker offset -->
    </sDEF>
    <s_category>
	  <ROOT>
		<TCEforms>
			<sheetTitle>Category, Targetgroup</sheetTitle>
		</TCEforms>
	    <type>array</type>
	    <el>
		  <categoryMode>
			<TCEforms>
				<label>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.select_deselect_categories</label>
				<config>
					<type>select</type>
					<items type="array">
						<numIndex index="0" type="array">
							<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.category_selection_showAll</numIndex>
							<numIndex index="1">0</numIndex>
						</numIndex>
						<numIndex index="1" type="array">
							<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.category_selection_showSelected</numIndex>
							<numIndex index="1">1</numIndex>
						</numIndex>
						<numIndex index="2" type="array">
							<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.category_selection_DontShowSelected</numIndex>
							<numIndex index="1">2</numIndex>
						</numIndex>
					</items>
					<maxitems>1</maxitems>
					<size>1</size>
				</config>
			</TCEforms>
	      	</categoryMode>
		  <categorySelection>
			<TCEforms>
				<label>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.category_selection</label>
				<config>
					<type>select</type>
					<items type="array">
						<!--
						<numIndex index="100">
								<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.category_selection_notCategorized</numIndex>
								<numIndex index="1">0</numIndex>
						</numIndex>
						-->
					</items>
					<foreign_table>tx_calendar_cat</foreign_table>
				    <!--
					<foreign_table_where>AND calendar_cat.pid=###STORAGE_PID### ORDER BY calendar_cat.uid</foreign_table_where>
				    -->
					<minitems>0</minitems>
					<maxitems>20</maxitems>
					<size>5</size>
					<selectedListStyle>width:150px</selectedListStyle>
					<itemListStyle>width:150px</itemListStyle>
					<allowNonIdValues>1</allowNonIdValues>
				</config>
			</TCEforms>
		  </categorySelection>

		  <targetgroupMode>
			<TCEforms>
				<label>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.select_deselect_targetgroup</label>
				<config>
					<type>select</type>
					<items type="array">
						<numIndex index="0" type="array">
							<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.targetgroup_selection_showAll</numIndex>
							<numIndex index="1">0</numIndex>
						</numIndex>
						<numIndex index="1" type="array">
							<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.targetgroup_selection_showSelected</numIndex>
							<numIndex index="1">1</numIndex>
						</numIndex>
						<numIndex index="2" type="array">
							<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.targetgroup_selection_DontShowSelected</numIndex>
							<numIndex index="1">2</numIndex>
						</numIndex>
					</items>
					<maxitems>1</maxitems>
					<size>1</size>
				</config>
			</TCEforms>
	      	</targetgroupMode>
		  <targetgroupSelection>
			<TCEforms>
				<label>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.targetgroup_selection</label>
				<config>
					<type>select</type>
					<items type="array">
						<!--
						<numIndex index="100">
								<numIndex index="0">LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.category_selection_notCategorized</numIndex>
								<numIndex index="1">0</numIndex>
						</numIndex>
						-->
					</items>
					<foreign_table>tx_calendar_targetgroup</foreign_table>
				    <!--
					<foreign_table_where>AND calendar_cat.pid=###STORAGE_PID### ORDER BY calendar_cat.uid</foreign_table_where>
				    -->
					<minitems>0</minitems>
					<maxitems>20</maxitems>
					<size>5</size>
					<selectedListStyle>width:150px</selectedListStyle>
					<itemListStyle>width:150px</itemListStyle>
					<allowNonIdValues>1</allowNonIdValues>
				</config>
			</TCEforms>
		  </targetgroupSelection>
	    </el>
	</ROOT>
    </s_category>

    <s_template>
	  <ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.sheet_template</sheetTitle>
		</TCEforms>
	    <type>array</type>
	    <el>
		<template_use_default>
		    <TCEforms>
			<label>Use default template for</label>
			    <config>
				<type>select</type>
				    <items type="array">
					<numIndex index="0">
					    <numIndex index="0">Use template file specified below</numIndex>
					    <numIndex index="1"></numIndex>
					</numIndex>
<!--
					<numIndex index="1">
					    <numIndex index="0">Year view</numIndex>
					    <numIndex index="1">month.html</numIndex>
					</numIndex>
-->
					<numIndex index="2">
					    <numIndex index="0">Month view</numIndex>
					    <numIndex index="1">month.html</numIndex>
					</numIndex>
					<numIndex index="3">
					    <numIndex index="0">Week view</numIndex>
					    <numIndex index="1">week.html</numIndex>
					</numIndex>
					<numIndex index="4">
					    <numIndex index="0">Day view</numIndex>
					    <numIndex index="1">day.html</numIndex>
					</numIndex>
					<numIndex index="5">
					    <numIndex index="0">Event view</numIndex>
					    <numIndex index="1">event.html</numIndex>
					</numIndex>
					<numIndex index="6">
					    <numIndex index="0">Upcoming view</numIndex>
					    <numIndex index="1">upcoming.html</numIndex>
					</numIndex>
					<numIndex index="7">
					    <numIndex index="0">Month mini view</numIndex>
					    <numIndex index="1">month-mini.html</numIndex>
					</numIndex>
<!--
					<numIndex index="8">
					    <numIndex index="0">Month summary</numIndex>
					    <numIndex index="1">month-summary.html</numIndex>
					</numIndex>
-->
				    </items>
			    </config>
			</TCEforms>
		</template_use_default>
	      <template_file>
			<TCEforms>
				<label>LLL:EXT:calendar/locallang_tca.php:calendar.pi_flexform.template_file</label>
				<config>
					<type>group</type>
					<internal_type>file</internal_type>
					<allowed>tmpl,html,htm</allowed>
					<max_size>100</max_size>
					<uploadfolder>uploads/tx_calendar</uploadfolder>
					<maxitems>1</maxitems>
					<size>1</size>
					<selectedListStyle>Width:180px</selectedListStyle>
				</config>
			</TCEforms>
	      </template_file>
	    </el>
	  </ROOT>
    </s_template>

