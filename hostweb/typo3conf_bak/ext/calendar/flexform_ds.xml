    <s_day>
	<ROOT>
	    <TCEforms>
		<sheetTitle>Day Config</sheetTitle>
	    </TCEforms>
	    <type>array</type>
	    <el>
		<interval>
		    <TCEforms>
			<label>Interval</label>
			<config>
			    <type>input</type>
			    <eval>num</eval>
			    <size>10</size>
			    <default>1800</default>
			    <range>
				<lower>0</lower>
				<upper>86400</upper>
			    </range>
			    <!-- XXX check this! -->
			    <required>true</required>
			</config>
		    </TCEforms>
		</interval>		 
	    </el>
	</ROOT>
    </s_day>

    <s_upcoming>
	<ROOT>
	    <TCEforms>
		<sheetTitle>Upcoming Config</sheetTitle>
	    </TCEforms>
	    <type>array</type>
	    <el>
		<how_many>
		    <TCEforms>
			<label>Display how many events?</label>
			<config>
			    <type>input</type>
			    <eval>num</eval>
			    <size>10</size>
			    <default>5</default>
			    <range>
				<lower>0</lower>
				<upper>1000</upper>
			    </range>
			</config>
		    </TCEforms>
		</how_many>		 
		<max_days>
		    <TCEforms>
			<label>Maximum days in future?</label>
			<config>
			    <type>input</type>
			    <eval>num</eval>
			    <size>10</size>
			    <default>365</default>
			    <range>
				<lower>0</lower>
				<upper>1000</upper>
			    </range>
			</config>
		    </TCEforms>
		</max_days>		 
	    </el>
	</ROOT>
    </s_upcoming>
  </sheets>
</T3DataStructure>
