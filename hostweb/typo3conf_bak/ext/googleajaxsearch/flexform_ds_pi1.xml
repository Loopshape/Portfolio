<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<T3DataStructure>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>Basic</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <apiKey>
            <TCEforms>
              <label>API Key (http://code.google.com/apis/ajaxsearch/signup.html)</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </apiKey>
          
          <linkTarget>
            <TCEforms>
              <label>Link Target</label>
                <config>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">Default</numIndex>
                    <numIndex index="1"></numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">Self</numIndex>
                    <numIndex index="1">self</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">Blank</numIndex>
                    <numIndex index="1">blank</numIndex>
                  </numIndex>
                  <numIndex index="3" type="array">
	                    <numIndex index="0">Top</numIndex>
	                    <numIndex index="1">top</numIndex>
	              </numIndex>
				<numIndex index="4" type="array">
					<numIndex index="0">Parent</numIndex>
					<numIndex index="1">parent</numIndex>
				</numIndex>
                </items>
                <type>select</type>
          		<minitems>0</minitems>
                <maxitems>1</maxitems>
              </config>
            </TCEforms>
          </linkTarget>
        </el>
      </ROOT>
    </sDEF> 
       
      <restrictions>
      <ROOT>
        <TCEforms>
          <sheetTitle>Search Restrictions</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>     
           <ff_siteRestriction>
            <TCEforms>
              <label>Sites To Search (space seperated domains)</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </ff_siteRestriction>
         <ff_titleWordRestriction>
            <TCEforms>
              <label>Title Keywords (space seperated keywords, quote phrases)</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </ff_titleWordRestriction>
          <ff_urlRestriction>
            <TCEforms>
              <label>URL Keywords (space seperated keywords)</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </ff_urlRestriction>
           <defaultSearch>
            <TCEforms>
              <label>Default Search Criteria</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </defaultSearch>
        </el>
      </ROOT>
    </restrictions> 
    
     <browserdomainmodel>
      <ROOT>
        <TCEforms>
          <sheetTitle>Browser DOM</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
        <ff_resultsDOMId>
            <TCEforms>
              <label>Results Id</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </ff_resultsDOMId>
          <!--ff_searchFormDOMId>
            <TCEforms>
              <label>Search Form Id</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </ff_searchFormDOMId>
          
          <ff_brandingDOMId>
            <TCEforms>
              <label>Branding Id</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </ff_brandingDOMId>
          <DOMPreposition>
            <TCEforms>
              <label>Preposition to all DOM Elements</label>
              <config>
                <type>input</type>
              </config>
            </TCEforms>
          </DOMPreposition-->
        
     </el>
      </ROOT>
    </browserdomainmodel> 
        
  </sheets>
</T3DataStructure>
