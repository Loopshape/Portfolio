<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
    <meta type="array">
        <description>Language labels for Newsletter un-/subscription</description>
    </meta>
    <data type="array">
        <languageKey index="default" type="array">

            <!-- FORM FIELDS -->
            <label index="gender.0">Mr</label>
            <label index="gender.0.val">m</label>
            <label index="gender.1">Mrs</label>
            <label index="gender.1.val">f</label>
            <label index="firstname">First name</label>
            <label index="lastname">Last name</label>
            <label index="email">E-Mail</label>
            <label index="emailunsub">E-Mail</label>
            <label index="company">Company</label>

            <label index="format">Format</label>
            <label index="format.0">HTML</label>
            <label index="format.0.val">1</label>
            <label index="format.1">Text</label>
            <label index="format.1.val">0</label>

            <label index="language">Language</label>
            <label index="language.0">German</label>
            <label index="language.0.val">0</label>
            <label index="language.1">English</label>
            <label index="language.1.val">1</label>

            <label index="subscribe">Subscribe</label>
            <label index="unsubscribe">Unsubscribe</label>

            <!-- ERROR MESSAGES -->
            <label index="error_email_required">Please enter your email address.</label>
            <label index="error_email_email">This is not a proper email address.</label>
            <label index="error_email_isNotInDBTable">This email address is already registered.</label>
            <label index="is_error_email_required">has-error</label>

            <!-- SUCCESS MESSAGE TEXTS -->
            <label index="success_message">Vielen Dank für Ihr Interesse. Bitte folgen Sie den Anweisungen in der E-Mail.</label>

            <!-- EMAIL TEXTS -->
            <label index="email_user_greetings">Hello</label>
            <label index="email_user_footer">company</label>
            <label index="email_user_footer_link">www.example.com</label>
            <!-- EMAIL TEXTS: Subscription -->
            <label index="email_user_subject">Newsletter subscription</label>
            <label index="email_user_text">Sie haben sich für unseren Newsletter angemeldet. Bitte klicken Sie auf den folgenden Link, um die Anmeldung abzuschließen:</label>

            <!-- UNSUBSCRIPTION -->
            <label index="error_email_isInDBTable">This email address is not registered.</label>
            <label index="unsub_email_user_subject">Newsletter unsubscription</label>
            <label index="unsub_email_user_text">Sie haben sich von unserem Newsletter abgemeldet. Dies tut uns leid. Beehren Sie uns bald wieder!</label>
            <label index="unsub_success_message">Sie haben sich erfolgreich von unserem Newsletter abgemeldet. Dies tut uns leid. Beehren Sie uns bald wieder!</label>


            <!-- NEWSLETTER LAYOUT -->

            <label index="newsletterLink2WebPart1">If this email is not displayed correctly,</label>
            <label index="newsletterLink2WebPart2">please click here to view it in a browser.</label>

        </languageKey>

