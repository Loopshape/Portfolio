      <target state="translated">Bearbeitungsverlauf anzeigen</target></trans-unit>
      <trans-unit id="time" xml:space="preserve" approved="yes">
        <source>Time</source>
      <target state="translated">Zeit</target></trans-unit>
      <trans-unit id="age" xml:space="preserve" approved="yes">
        <source>Age</source>
      <target state="translated">Alter</target></trans-unit>
      <trans-unit id="user" xml:space="preserve" approved="yes">
        <source>User</source>
      <target state="translated">Benutzer</target></trans-unit>
      <trans-unit id="tableUid" xml:space="preserve" approved="yes">
        <source>Table:uid</source>
      <target state="translated">Tabelle:Uid</target></trans-unit>
      <trans-unit id="differences" xml:space="preserve" approved="yes">
        <source>Differences</source>
      <target state="translated">Unterschiede</target></trans-unit>
      <trans-unit id="changes" xml:space="preserve" approved="yes">
        <source>Changelog</source>
      <target state="translated">Änderungen</target></trans-unit>
      <trans-unit id="return" xml:space="preserve" approved="yes">
        <source>Return</source>
      <target state="translated">Zurück</target></trans-unit>
      <trans-unit id="returnLink" xml:space="preserve" approved="yes">
        <source>Click here to go back</source>
      <target state="translated">Klicken Sie hier, um zurückzukehren</target></trans-unit>
      <trans-unit id="revertAll" xml:space="preserve" approved="yes">
        <source>Rollback all changes shown</source>
      <target state="translated">Alle gezeigten Änderungen rückgängig machen</target></trans-unit>
      <trans-unit id="revertRecord" xml:space="preserve" approved="yes">
        <source>Rollback single record</source>
      <target state="translated">Einzelnen Eintrag rückgängig machen</target></trans-unit>
      <trans-unit id="revertField" xml:space="preserve" approved="yes">
        <source>Rollback single field</source>
      <target state="translated">Feld wiederherstellen</target></trans-unit>
      <trans-unit id="externalChange" xml:space="preserve" approved="yes">
        <source>Intermediate external change!</source>
      <target state="translated">Externe Änderung!</target></trans-unit>
      <trans-unit id="sumUpChanges" xml:space="preserve" approved="yes">
        <source>Rollback (Preview)</source>
      <target state="translated">Rückgängig machen (Vorschau)</target></trans-unit>
      <trans-unit id="markState" xml:space="preserve" approved="yes">
        <source>mark this change</source>
      <target state="translated">Diese Änderung markieren</target></trans-unit>
      <trans-unit id="unmarkState" xml:space="preserve" approved="yes">
        <source>unMark this change</source>
      <target state="translated">Markierung der Änderung löschen</target></trans-unit>
      <trans-unit id="elementHistory" xml:space="preserve" approved="yes">
        <source>You are currently seeing the history for an element.</source>
      <target state="translated">Derzeit wird der Verlauf für ein Element angezeigt.</target></trans-unit>
      <trans-unit id="elementHistory_link" xml:space="preserve" approved="yes">
        <source>Show the full page history!</source>
      <target state="translated">Den kompletten Seitenverlauf anzeigen</target></trans-unit>
      <trans-unit id="linkRecordHistory" xml:space="preserve" approved="yes">
        <source>Limit history to this record</source>
      <target state="translated">Verlauf auf diesen Datensatz begrenzen</target></trans-unit>
      <trans-unit id="differenceMsg" xml:space="preserve" approved="yes">
        <source>In the Difference column you can see the changes at every modification of records. New changes are displayed in &lt;span class="diff-g"&gt;green colored text&lt;/span&gt; and the &lt;span class="diff-r"&gt;red colored text&lt;/span&gt; are the old values which were removed.</source>
      <target state="translated">In der Spalte Unterschied sind die Änderungen bei jeder Bearbeitung des Datensatzes zu sehen. Neue Änderungen werden mit &lt;span class="diff-g"&gt;grünem Text&lt;/span&gt; dargestellt, die Änderungen mit &lt;span class="diff-r"&gt;rotem Text&lt;/span&gt; sind alte Werte, die entfernt wurden.</target></trans-unit>
      <trans-unit id="fullView" xml:space="preserve" approved="yes">
        <source>Return to full view</source>
      <target state="translated">Zurück zur Gesamtanzeige</target></trans-unit>
      <trans-unit id="insert" xml:space="preserve" approved="yes">
        <source>insert</source>
      <target state="translated">einfügen</target></trans-unit>
      <trans-unit id="delete" xml:space="preserve" approved="yes">
        <source>delete</source>
      <target state="translated">löschen</target></trans-unit>
      <trans-unit id="mergedDifferences" xml:space="preserve" approved="yes">
        <source>Preview for Rollback</source>
      <target state="translated">Vorschau der Wiederherstellung</target></trans-unit>
      <trans-unit id="settings" xml:space="preserve" approved="yes">
        <source>Settings</source>
      <target state="translated">Einstellungen</target></trans-unit>
