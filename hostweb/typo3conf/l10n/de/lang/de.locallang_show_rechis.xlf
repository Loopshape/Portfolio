        <source>ALL</source>
      <target state="translated">ALLE</target></trans-unit>
      <trans-unit id="maxSteps_marked" xml:space="preserve" approved="yes">
        <source>marked</source>
      <target state="translated">markiert</target></trans-unit>
      <trans-unit id="showDiff" xml:space="preserve" approved="yes">
        <source>Show differences:</source>
      <target state="translated">Unterschiede anzeigen:</target></trans-unit>
      <trans-unit id="showDiff_no" xml:space="preserve" approved="yes">
        <source>No</source>
      <target state="translated">Nein</target></trans-unit>
      <trans-unit id="showDiff_inline" xml:space="preserve" approved="yes">
        <source>Inline</source>
      <target state="translated">Eingebettet</target></trans-unit>
      <trans-unit id="showSubElements" xml:space="preserve" approved="yes">
        <source>Show sub elements:</source>
      <target state="translated">Unterelemente anzeigen:</target></trans-unit>
      <trans-unit id="no" xml:space="preserve" approved="yes">
        <source>No</source>
      <target state="translated">Nein</target></trans-unit>
      <trans-unit id="yes" xml:space="preserve" approved="yes">
        <source>Yes</source>
      <target state="translated">Ja</target></trans-unit>
      <trans-unit id="showInsertDelete" xml:space="preserve" approved="yes">
        <source>Show inserts/deletes of records:</source>
      <target state="translated">Eingefügte/gelöschte Datensätze anzeigen</target></trans-unit>
      <trans-unit id="noDifferences" xml:space="preserve" approved="yes">
        <source>There are no differences to the current page!</source>
      <target state="translated">Es liegen keine Unterschiede zur aktuellen Seite vor!</target></trans-unit>
